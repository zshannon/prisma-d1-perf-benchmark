{
  "dependencies": {
    "@prisma/adapter-d1": "5.21.0",
    "@prisma/client": "5.21.0",
    "next": "14.2.15",
    "react": "18",
    "react-dom": "18"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "4.20241011.0",
    "@types/node": "20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/xo": "0",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-xo": "0.46.0",
    "eslint-config-xo-react": "0.27.0",
    "eslint-plugin-next-on-pages": "1.13.3",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "prisma": "5.21.0",
    "typescript": "5",
    "vercel": "37.8.0",
    "wrangler": "3.80.5",
    "xo": "0.59.3"
  },
  "name": "prisma-d1-perf-benchmark",
  "private": true,
  "scripts": {
    "build": "next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "db:migrate": "wrangler d1 migrations create prisma-d1-perf-benchmark $@ && prisma migrate diff --from-local-d1 --to-schema-datamodel ./prisma/schema.prisma --script --output migrations/$(ls migrations | sort -f | tail -n 1)",
    "db:migrate:init": "prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script --output migrations/$(ls migrations | sort -f | tail -n 1)",
    "db:migrate:push": "wrangler d1 migrations apply prisma-d1-perf-benchmark --remote",
    "db:migrate:up": "wrangler d1 migrations apply prisma-d1-perf-benchmark --local && prisma generate",
    "deploy": "yarn pages:build && wrangler pages deploy",
    "dev": "next dev",
    "lint": "next lint",
    "pages:build": "yarn next-on-pages",
    "postinstall": "prisma generate",
    "preview": "yarn pages:build && wrangler pages dev",
    "start": "next start"
  },
  "version": "0.1.0"
}
